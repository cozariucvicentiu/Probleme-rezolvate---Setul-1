using System;
namespace vice0
{
    class Program
    {
        static void Main(string[] args)
        {int y=int.Parse(Console.ReadLine());
            if((y%400==0) || (y%4==0 && y%100 !=0))
            { Console.WriteLine(y + " este an bisect"); }
            else { Console.WriteLine(y + " nu este an bisect"); }
            Console.ReadKey();
        }
    }
}
